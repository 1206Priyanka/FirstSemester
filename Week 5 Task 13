basenumber = 0
data = {}
while basenumber < 5:
    basenumber = basenumber+1
    exponent = 0
    while exponent < 5:
        exponent = exponent+1
        result = basenumber**exponent
        if basenumber not in data:  
            data[basenumber] = [result]
        else:
            data[basenumber].append(result)

print(data)

        
